import java.util.List

public class lecturer {
    private String name;
    private List<String> modulesTeaching;

    public lecturer(){

    }
    public lecturer(String name, List<String> modulesTeaching) {
        this.name = name;
        this.modulesTeaching = modulesTeaching;
    }

    public String getname {return name};

    public List<String> getModulesTeaching { return modules};

    public void viewTimetable() {
        System.out.println("Lecturer: " + name);
        System.out.println("Modules teaching:");

        if (modulesTeaching == null || modulesTeaching.isEmpty()) {
            System.out.println("No modules assigned");
        } else {
            for (String module : modulesTeaching) {
                System.out.println(module);
            }
        }
    }
}